public class SyncTest3 {

    private int x = 1;
    private Object y;

    public void test1() {
        synchronized (this) {
            try {
                if (x == 4) return;
                synchronized (y) {
                    x = 5;
                }
                x = 3;
                return;
            } catch (RuntimeException e) {
                x = 4;
            }
        }
    }
}
